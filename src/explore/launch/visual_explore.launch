<launch>
  <arg name="parent_frame"  default="/map"/>
  <arg name="child_frame" default="/camera_link"/>
  <arg name="pose_name" default="/cur_camera_pose"/> <!--rostopic type : geometry_msgs::PoseStamped-->
  <arg name="rate" default="100"/>

  <node name="establishtf_node" pkg="establishtf"  type="establishtf_node" 
        args = "$(arg parent_frame) $(arg child_frame) $(arg pose_name)  $(arg rate)" output="screen"/>

  <arg name="cloud_name" default="/cur_kp_cloud"/>  <!--rostopic type : sensor_msgs::PointCloud-->
  <arg name="obs_count"  default="100"/>
  <arg name="obs_cost"  default="0.8"/>
  <arg name="shooting_dst"  default="0.5"/>
  <arg name="cam_width"  default="0.64"/>
  <arg name="cam_height"  default="0.02"/>
  <arg name="resolution" default="0.02"/>
  <arg name="kdtree_radius" default="0.1"/>
  <arg name="kdtree_K" default="0"/>
  <arg name="cost_scaling_factor" default="10.0"/>
  <arg name="trapezoid_hei" default="0.0"/>
  <arg name="trapezoid_len" default="0.0"/>
  <arg name="dst_filter_factor" default="0.1"/>

  <node name="explore_node" pkg="explore"  type="explore_node" 
        args = "$(arg cloud_name) $(arg pose_name) $(arg obs_count) $(arg obs_cost) $(arg shooting_dst) $(arg cam_width) $(arg cam_height) $(arg resolution) $(arg dst_filter_factor)  $(arg cost_scaling_factor)" output="screen"/>

</launch>